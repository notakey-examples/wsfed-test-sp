@page
@model PrivateModel
@{
    ViewData["Title"] = "Issued Claims";
}
<h1>@ViewData["Title"]</h1>

@if (User?.Identity?.IsAuthenticated ?? false) {
    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
}